<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<Pane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="700.0" prefWidth="1200.0" stylesheets="@ffxiv.css"
      xmlns="http://javafx.com/javafx/8.0.111" fx:controller="io.github.krindor.ffxivsimulator.JavaFX.NinjaSimController">
    <children>
        <TabPane prefHeight="700.0" prefWidth="1200.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="Simulator">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <AnchorPane fx:id="anchorPaneSim" layoutY="-29.0" onMouseDragged="#draggedMove"
                                            onMousePressed="#pressedMove" prefHeight="700.0" prefWidth="1200.0"
                                            stylesheets="@ffxiv.css">
                                    <children>
                                        <Button layoutX="24.0" layoutY="636.0" mnemonicParsing="false"
                                                onAction="#mainSceneChanger" prefHeight="50.0" prefWidth="140.0"
                                                text="Main Menu">
                                            <cursor>
                                                <Cursor fx:constant="HAND"/>
                                            </cursor>
                                        </Button>
                                        <Button layoutX="188.0" layoutY="636.0" mnemonicParsing="false"
                                                onAction="#CharSceneChanger" prefHeight="50.0" prefWidth="140.0"
                                                text="Character Select">
                                            <cursor>
                                                <Cursor fx:constant="HAND"/>
                                            </cursor>
                                        </Button>
                                        <TextField fx:id="weaponText" layoutX="131.0" layoutY="65.0" text="85"/>
                                        <TextField fx:id="dexText" layoutX="131.0" layoutY="102.0" text="1589"/>
                                        <TextField fx:id="critText" layoutX="131.0" layoutY="138.0" text="1366"/>
                                        <TextField fx:id="detText" layoutX="131.0" layoutY="178.0" text="689"/>
                                        <TextField fx:id="ssText" layoutX="131.0" layoutY="212.0" text="444"/>
                                        <Label layoutX="14.0" layoutY="69.0" text="Weapon Damage"/>
                                        <Label layoutX="14.0" layoutY="106.0" text="Dexterity"/>
                                        <Label layoutX="14.0" layoutY="142.0" text="Crit"/>
                                        <Label layoutX="14.0" layoutY="182.0" text="Determination"/>
                                        <Label layoutX="14.0" layoutY="216.0" text="Skill Speed"/>
                                        <Button layoutX="725.0" layoutY="636.0" mnemonicParsing="false"
                                                onAction="#startSim" prefHeight="50.0" prefWidth="140.0" text="Run Sim">
                                            <cursor>
                                                <Cursor fx:constant="HAND"/>
                                            </cursor>
                                        </Button>
                                        <Button layoutX="357.0" layoutY="636.0" mnemonicParsing="false"
                                                onAction="#writeStringToFile" prefHeight="50.0" prefWidth="140.0"
                                                text="Save">
                                            <cursor>
                                                <Cursor fx:constant="HAND"/>
                                            </cursor>
                                        </Button>
                                        <ProgressBar layoutX="996.0" layoutY="662.0" prefHeight="21.0" prefWidth="175.0"
                                                     progress="0.72"/>
                                        <Label layoutX="892.0" layoutY="614.0" prefHeight="45.0" prefWidth="64.0"
                                               text="DPS:" textFill="#ee1818">
                                            <font>
                                                <Font size="25.0"/>
                                            </font>
                                        </Label>
                                        <Label layoutX="892.0" layoutY="654.0" text="Progress">
                                            <font>
                                                <Font size="25.0"/>
                                            </font>
                                        </Label>
                                        <Label layoutX="996.0" layoutY="614.0" prefHeight="30.0" prefWidth="141.0"
                                               text="2000">
                                            <font>
                                                <Font size="30.0"/>
                                            </font>
                                        </Label>
                                        <Button layoutX="397.0" layoutY="187.0" mnemonicParsing="false"
                                                onAction="#customizeOpener" prefHeight="50.0" prefWidth="140.0"
                                                text="Customize Opener">
                                            <cursor>
                                                <Cursor fx:constant="HAND"/>
                                            </cursor>
                                        </Button>
                                        <Label layoutX="286.0" layoutY="142.0" text="Simulation Type"/>
                                        <MenuButton fx:id="simulationType" layoutX="397.0" layoutY="138.0"
                                                    mnemonicParsing="false" prefHeight="25.0" prefWidth="140.0"
                                                    text="Simulation Type">
                                            <items>
                                                <MenuItem mnemonicParsing="false" onAction="#setOpener" text="Default"/>
                                                <MenuItem mnemonicParsing="false" onAction="#setOpener"
                                                          text="Custom Opener"/>
                                            </items>
                                        </MenuButton>
                                    </children>
                                </AnchorPane>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Results">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <TextArea fx:id="damageLog" layoutX="53.0" layoutY="52.0" prefHeight="577.0"
                                          prefWidth="475.0"/>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Settings">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <CheckBox fx:id="warrior" layoutX="150.0" layoutY="184.0" mnemonicParsing="false"
                                          text="Warrior in party"/>
                                <TextField fx:id="simTimeText" layoutX="150.0" layoutY="61.0" text="180"/>
                                <TextField fx:id="hutonText" layoutX="150.0" layoutY="102.0" text="23"/>
                                <Label layoutX="48.0" layoutY="65.0" text="Simulation Time"/>
                                <Label layoutX="29.0" layoutY="106.0" text="Huton Pre-pull time"/>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
            <cursor>
                <Cursor fx:constant="DEFAULT"/>
            </cursor>
        </TabPane>
        <Pane fx:id="barForGrab" layoutX="184.0" prefHeight="33.0" prefWidth="1016.0">
            <children>
                <Label layoutX="1002.0" layoutY="8.0" onMousePressed="#closeProgram" text="X"/>
                <Label layoutX="979.0" layoutY="-12.0" onMousePressed="#minimize" prefHeight="41.0" prefWidth="8.0"
                       text="-">
                    <font>
                        <Font size="36.0"/>
                    </font>
                </Label>
            </children>
        </Pane>
    </children>
</Pane>
